    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "yarn-audit-fix": "^9.3.2"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,css,md,ts,tsx,json,yml,yaml}": "prettier --write"
